local CINCO_DE_MAYO = 75654509883757
local APRIL_FOOLS = 135050118610638

local getToggleLogo = function()
local logo = getHolidayLogo()
local toglogo;
if logo == "🎄" then toglogo = XMAS
elseif logo == "🦃" then toglogo = THANKSGIVING
elseif logo == "🎃" then toglogo = HALLOWEEN
elseif logo == "💝" then toglogo = VALENTINES
elseif logo == "🍬" then toglogo = LOLLYPOP
elseif logo == "🎆" then toglogo = NEWYEARS
elseif logo == "🇲🇽" then toglogo = LOLLYPOP
elseif logo == "🎂" then toglogo = LOLLYPOP
elseif logo == "🤡" then toglogo = APRIL_FOOLS
elseif logo == "🍀" then toglogo = STPATRICKS
else toglogo = LOLLYPOP;
end;
return toglogo;
end;

local getToggleSize = function()
local tog = getToggleLogo()
local togsize
if tog == XMAS then togsize = 83
elseif tog == THANKSGIVING then togsize = 60
elseif tog == HALLOWEEN then togsize = 70
elseif tog == VALENTINES then togsize = 61
elseif tog == LOLLYPOP then togsize = 100
elseif tog == NEWYEARS then togsize = 59
elseif tog == STPATRICKS then togsize = 76
elseif tog == CINCO_DE_MAYO then togsize = 66
elseif tog == BIRTHDAY then togsize = 100
elseif tog == APRIL_FOOLS then togsize = 87
else togsize = 100
end;
return togsize;
end;
