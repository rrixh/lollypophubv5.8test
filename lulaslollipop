--// FE SKRIPTS \\--

local Fe = Window:NewTab("FE")
local FeSection = Fe:NewSection("FE Skripts");
fe, FE = FeSection;


fe:NewButton("FE Jerk off", "sex", function()
local jerkoff,err=pcall(function()
