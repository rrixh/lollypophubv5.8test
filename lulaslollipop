uis.JumpRequest:connect(function()
if getgenv().InfJump == true then
niggas:GetService"Players".LocalPlayer.Character:FindFirstChildOfClass'Humanoid':ChangeState("Jumping")
end
end)
wait()
end
end)
else
getgenv().InfJump = false
end
end)
