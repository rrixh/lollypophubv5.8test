local function enablestuff()
for i,v in pairs(Players:GetPlayers()) do
if v~= plr and v.Character then
if not v.Character:FindFirstChild('ESP') then
local highlight = Instance.new('Highlight')
highlight.Name = 'ESP'
highlight.Parent = v.Character
highlight.Enabled = getgenv().Enabled
highlight.FillColor = getgenv().espstuff
highlight.OutlineColor = getgenv().espstuff
else
local highlight = v.Character:FindFirstChild('ESP')
highlight.Enabled = getgenv().Enabled
highlight.FillColor = getgenv().espstuff
highlight.OutlineColor = getgenv().espstuff
end
end
end
end

RunService.RenderStepped:connect(function()
enablestuff()
end)


