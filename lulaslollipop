-- [ TOGGLE klose/open ] --
local function TOGGLE2()
--getgenv().togglesize = 82;
--getgenv().getImg = 17767487755
local screenGui = Instance.new("ScreenGui")
screenGui.Parent = game.CoreGui
screenGui.IgnoreGuiInset = true
local removeborder = 0
local overlayAllGui = 2
local roundButton = Instance.new("ImageButton")
roundButton.Parent = screenGui
roundButton.Size = UDim2.new(0, togglesize, 0, togglesize) -- Set the size of the button
roundButton.Position = UDim2.new(1, -110, 0, 25) -- Position on the upper right (110 pixels left from the right edge, 10 pixels down from the top)
roundButton.Image = "http://www.roblox.com/asset/?id="..getgenv().getImg
roundButton.BackgroundTransparency = 1 -- Set transparency so only the image is shown
roundButton.ImageColor3 = Color3.fromRGB(255, 255, 255) -- Set image color
roundButton.ScaleType = Enum.ScaleType.Fit;
roundButton.BorderSizePixel = removeborder
roundButton.ZIndex = overlayAllGui
-- Make the button round
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(1, 0) -- Full radius to make it round
uiCorner.Parent = roundButton;
--togglefunk
local function toggleKavo()
KAVO:ToggleUI()
end;
--wen pressed will toggle ui
roundButton.MouseButton1Click:Connect(toggleKavo);
-- so it wont run on non mobile devixe
local uis=game:GetService("UserInputService")
if not uis.TouchEnabled then
roundButton.Visible = true
end;
end;--togglelollypop funk(not using)

--===========LH TOGGLE==============--
local function KUSTOMTOGGLE()
loadstring(game:HttpGet("https://raw.githubusercontent.com/rrixh/uwuware/main/lib/kavomobile.lula",true))();
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "KustomToggle"
screenGui.Parent = game.CoreGui
screenGui.IgnoreGuiInset = true
local removeborder = 0
local overlayAllGui = 5
local roundButton = Instance.new("ImageButton")
roundButton.Parent = screenGui
roundButton.Size = UDim2.new(0, togglesize, 0, togglesize)
roundButton.Position = UDim2.new(1, -110, 0, 25)
roundButton.Image = "http://www.roblox.com/asset/?id="..getImg
roundButton.BackgroundTransparency = 1
roundButton.ImageColor3 = Color3.fromRGB(255, 255, 255)
roundButton.ScaleType = Enum.ScaleType.Fit
roundButton.BorderSizePixel = removeborder
roundButton.ZIndex = overlayAllGui;
--roundbtn
local uiCorner = Instance.new("UICorner")
uiCorner.CornerRadius = UDim.new(1, 0)
uiCorner.Parent = roundButton;
local function toggleKavo() 
KAVO:ToggleUI();
end

local ______ = spawn;local fix = pcall;
-- dragging
local dragging = false
local dragInput, mousePos, framePos
local dragThreshold = 5 --wont tap by mistake
local hasMoved = false;

local function update(input)
local delta = input.Position - mousePos
if math.abs(delta.X) > dragThreshold or math.abs(delta.Y) > dragThreshold then
    hasMoved = true
    roundButton.Position = UDim2.new(framePos.X.Scale, framePos.X.Offset + delta.X, framePos.Y.Scale, framePos.Y.Offset + delta.Y)
end
end;

roundButton.InputBegan:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
    dragging = true
    hasMoved = false
    mousePos = input.Position
    framePos = roundButton.Position;

    input.Changed:Connect(function()
        if input.UserInputState == Enum.UserInputState.End then
            dragging = false
        end
    end)
end
end);
roundButton.InputChanged:Connect(function(input)
if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
    dragInput = input
end
end);
game:GetService("UserInputService").InputChanged:Connect(function(input)
if input == dragInput and dragging then
    update(input)
end
end);
-- wont tap if dragging
roundButton.MouseButton1Click:Connect(function()
if not hasMoved then
    toggleKavo()
end
end)
end;--end KUSTOM TOGGLE FUNK
-- so it wont run on non-mobile devices
--[[
local uis = game:GetService("UserInputService")
if not uis.TouchEnabled then
roundButton.Visible = true
end
]]--

game.Players.LocalPlayer.Chatted:Connect(function(...)
if (...) == '/e toggle' then
KUSTOMTOGGLE()
end
end)
--============END TOGGLE===========--

spawn(KUSTOMTOGGLE);
spawn(MAIN);

-- spawn(MAINE)

--loading skreen
end;
spawn(animateSkript);

local LOADED = tick()-START
local LoLLyPoP=true

repeat wait() until(LoLLyPoP~=nil)

if(LoLLyPoP~=nil)then
warn("Lollypop Hub🍭 loaded in "..string.format("%.2f", LOADED).." sekonds✅")
end;