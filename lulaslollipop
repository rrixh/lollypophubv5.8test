-- toggle anti sit
SkriptsSection:NewToggle("Anti-Sit", "kinda good", function(state)
if state then
local RunService = game:GetService("RunService")
function antiSit()
if human:GetState() == Enum.HumanoidStateType.Seated then 
human.Sit = false
end
end
RunService:BindToRenderStep("tempBinding", 1000, antiSit)

game:GetService("StarterGui"):SetCore("SendNotification",{ 	
Title = "Anti-Sit✅",  	
Text = "on",

Duration = 3
})
else
local RunService = game:GetService("RunService")
function antiSit()
if game.Players.LocalPlayer.Character.Humanoid:GetState() == Enum.HumanoidStateType.Seated then 
game.Players.LocalPlayer.Character.Humanoid.Sit = true
end
end
RunService:BindToRenderStep("tempBinding", 1000, antiSit)

game:GetService("StarterGui"):SetCore("SendNotification",{ 	
Title = "Anti-Sit❌",  	
Text = "off",

Duration = 3
})
end
end)
--end toggle antisit


