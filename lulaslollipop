local function milk(kookie)
loadingBar.Size = UDim2.new(kookie / 105, 0, 0.2, 0)
loadingText.Text = "Skript Loading: " .. kookie .. "%"
end;
--animateloadingbar
local function animateSkript()
local kookie=0;
while kookie < 100 do
kookie = kookie + loadSpeed
milk(kookie)
wait(0.07)-- speed
end;

loadingText.Text = "Suxxessfully Loaded 😛"
wait(0.1)
