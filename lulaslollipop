game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Lollypop Hub 🍭", Duration = 4.5 or dur, Button1 = "bet" or btn, Callback = function()print('btn')end, Text = text});
end;ok=bNotify;n=bNotify;

getgenv().write=function(msg,typingtime)
spawn(function()
local h=Instance.new("Hint");
h.Parent = game.Workspace;
local function typeMessage()
for i=1,#msg do h.Text=string.sub(msg,1,i);
wait(typingtime); end;end;
typeMessage();wait(3.3);h:Destroy(); end)
end;

