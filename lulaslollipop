bill.Size = UDim2.new(4, 0, 4.5, 0)
bill.AlwaysOnTop = true
local label = Instance.new("TextLabel", bill)
label.Size = UDim2.new(2, 0, 1, 0) 
                label.Position = UDim2.new(-0.5, 0, -0.5, 0)              label.BackgroundTransparency = 1         label.FontSize = "Size14";
while true do
label.TextColor3 = Rainbow
wait(.09)                               label.TextStrokeColor3 = Color3.new(0 ,0 ,0)                    label.TextStrokeTransparency = none
label.Text = (...)
if label.Text == "" or label.Text == " " then
wait();
bill:Destroy();
          break
end;
end
end)

