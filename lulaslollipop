local getHolidayLogo = function()
local month, day = os.date("*t").month, os.date("*t").day

if month == 12 and (day >= 5 and day <= 26) then  -- Christmas (Dec 5-26)
holidayLogo = "🎄"
elseif month == 11 and (day >= 20 and day <= 30) then -- Thanksgiving (Nov 20-30)
holidayLogo = "🦃"
elseif month == 10 and (day >= 20 and day <= 31) then -- Halloween (Oct 24-31)
holidayLogo = "🎃"
elseif (month == 2 and day == 14) or (month == 2 and (day >= 7 and day <= 14)) then -- Valentine's Day
holidayLogo = "💝"
elseif (month == 1 and day == 1) or (month == 12 and (day >= 26 and day <= 31)) then -- New Year's (Jan 1, Dec 26-31)
holidayLogo = "🎆"
elseif month == 3 and (day >= 9 and day <= 17) then -- st. patrik day (Mar 11-17)
holidayLogo = "🍀"
elseif month == 5 and (day >= 1 and day <= 5) then -- Cinco de Mayo (May 1-5)
holidayLogo = "🇲🇽"
elseif month == 6 and day == 2 then -- Birthday
holidayLogo = "🎂"
elseif (month == 4 and day == 1) or (month == 3 and (day >= 26 and day <= 30)) then
holidayLogo = "🤡"
elseif (month == 4 and day == 20) then
holidayLogo = "🍃"
else
holidayLogo = "🍬"
end
return holidayLogo
end;

local XMAS = 83624080780174
local THANKSGIVING = 124622552375747
