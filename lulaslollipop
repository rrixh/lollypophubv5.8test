until game:IsLoaded()

local player = game.Players.LocalPlayer
local nigguh = player.Character

local part = Instance.new("Part", nigguh)
part.Transparency = 1
part.Anchored = true
part.CanCollide = false

local tweenService = game:GetService("TweenService")
local runService = game:GetService("RunService")
local kamera = workspace.CurrentCamera

kamera.CameraSubject = part

runService.RenderStepped:Connect(function()
local FAXEKAM = tweenService:Create(part, TweenInfo.new(2, Enum.EasingStyle.Back), {CFrame = character.HumanoidRootPart.CFrame * CFrame.new(0, 2, 0)}) --offset
FAXEKAM:Play();
end);
end)

