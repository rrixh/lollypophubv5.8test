local function updatetimelabel()
while true do
local formatted_time = os.date("üî¥Live Time: %I:%M:%S %p")
local todays_date = os.date("Today's Date: %A | %B %d, %Y");

--‚è∞////%I=12 Hour; %M=Minute; %S=Second; %p=AM/PM; %A=Day of week; %d=Day of month; %m=Month; %Y=Year\\\\‚è∞

time:UpdateLabel("[" ..formatted_time.."]\n ["..todays_date.."]")
wait(0.001)
end
end

