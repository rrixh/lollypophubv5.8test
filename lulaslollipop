HomeSection:NewButton("Open Dev Konsole", "see roblox dev konsole",
function()
_G.sgui:SetCore("DevConsoleVisible",true)
end)

HomeSection:NewButton("Rejoin", "rejoins the same lobby",
function()
--rejoin lobby
local ts = game:GetService("TeleportService");
local p = game:GetService("Players").LocalPlayer;
--rj
ts:TeleportToPlaceInstance(game.PlaceId, game.JobId, p)
end)
HomeSection:NewButton("Server Hop", "leave the game",
function()
loadstring(game:HttpGet("https://github.com/rrixh/skripts/raw/main/serverhop",true))();
end)
HomeSection:NewButton("Join Lowest Player Server", "makes u join same game but lowest player",
function()
wait()
local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/FilteringEnabled/FE/main/NotificationUI"))();
local Notify = AkaliNotif.Notify;
wait();
Notify({
Description = "Finding smallest server...";
Title = "Lollypop Hub🍭";
Duration = 2;
});
task.spawn(function()
--low server
loadstring(game:HttpGet("https://github.com/rrixh/skripts/raw/main/joinsmallestserver",true))();
end)
end)
HomeSection:NewButton("Exit Game", "leave the game",
function()game:Shutdown()
end)

HomeSection:NewTextBox("Walkspeed","xhanges the walkspeed", function(speed)
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed=speed;
end)

HomeSection:NewTextBox("Jump Power","xhanges the jumppower", function(jp)
game.Players.LocalPlayer.Character.Humanoid.JumpPower=jp
-- jumpheight
end)

HomeSection:NewButton("Reset Walkspeed","reset walkspeed to default", function()game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

HomeSection:NewButton("Reset Walkspeed/Jumppower", "resets Walkspeed and jumpPower to default", function()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
