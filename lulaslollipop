local selektedPlayerName = nil;
local function ALLPLRS()
local players = game.Players:GetPlayers()
local playerNames = {}
for _, player in ipairs(players) do
table.insert(playerNames, player.Name)
end
return playerNames
end;

local playersInServer = SkriptsSection:NewDropdown("Selekt Player", "xhoose a player from the list", ALLPLRS(), function(selektedPlayer)
selektedPlayerName = selektedPlayer
end);

local function updatePlayerList()
playersInServer:Refresh(ALLPLRS())
playerToolStealer:Refresh(ALLPLRS())
end;

--handle plrs  being added and removed
game.Players.PlayerAdded:connect(updatePlayerList)
game.Players.PlayerRemoving:connect(updatePlayerList);

SkriptsSection:NewButton("Teleport to Selekted Player", "tp to player", function()
if selektedPlayerName then
local player = game.Players.LocalPlayer
local targetPlayer = game.Players:FindFirstChild(selektedPlayerName);
if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
    local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
player.Character.HumanoidRootPart.CFrame = CFrame.new(targetPosition)
        print("“"..selektedPlayerName.."“")
    else
        bNotif(3, "ok", "Player or HumanoidRootPart not found")
    end
else
    bNotif(3, "aight", "Player or their HumanoidRootPart not found")
end
else
bNotif(3, "oh", "No player selekted")
end;
end)

local TweenService = game:GetService("TweenService")

SkriptsSection:NewButton("Tween to Selekted Player", "tp to player", function()
if selektedPlayerName then
local player = game.Players.LocalPlayer
local targetPlayer = game.Players:FindFirstChild(selektedPlayerName)
if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
    local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
    if player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = player.Character.HumanoidRootPart
        local targetCFrame = CFrame.new(targetPosition)
        local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
        local tween = TweenService:Create(humanoidRootPart, tweenInfo, {CFrame = targetCFrame})
        tween:Play()
        tween.Completed:Connect(function()
print("“"..selektedPlayerName.."“")
        end)
    else
        bNotif(3, "aight", "Player or HumanoidRootPart not found.")
    end
else
    bNotif(3, "aight", "Player or their HumanoidRootPart not found")
end
else
bNotif(2, "oops im dumb", "No player selekted")
end;
end)

