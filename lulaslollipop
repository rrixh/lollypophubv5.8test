local function updatetimelabel()
while true and wait(0.2) do
local formatted_time = os.date("🕰 Live Time: %I:%M:%S %p")
local todays_date = os.date("Today's Date: %A | %B %d, %Y");

--⏰////%I=12 Hour; %M=Minute; %S=Second; %p=AM/PM; %A=Day of week; %d=Day of month; %m=Month; %Y=Year\\\\⏰

time:UpdateLabel("[" ..formatted_time.."]\n ["..todays_date.."]")
end
end

