if not setfflag then _G.note("your executor doesnt support setfflag for FPS boosting") return end;
if boostfps == true then
boostfps = false 
else 
boostfps = true; 
end;

if boostfps == true then
_G.boost = _G.hb:connect(function()
setfflag("TaskSchedulerTargetFps", 125);
end)
_G.note("boosting fps", 2)
        end;
if boostfps == false then
_G.boost:Disconnect(); _G.boost=nil;
_G.note("stopped boosting fps", 2)
       end;
end)

