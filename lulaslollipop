local Tools = Window:NewTab("Tools")
local ToolsSection = Tools:NewSection("Tools")
toolz=ToolsSection;

toolz:NewButton("Backpack Tool", "use tools frm players", function()
setclipboard([[
getgenv().ToggleButton = true
owner = "rrixh"
repo = "uwuware"
branxhk = (branxhk ~= nil and branxhk ~= "") or "main"
getgenv().webimport=function(file)
url=string.format("https://raw.githubusercontent.com/%s/%s/%s/%s", owner, repo, branxhk, file)
    skript=game:HttpGet(url)
    return loadstring(skript,file)()
end;
webimport("kustomskript/Baxkpaxktool.lua")
task.wait(1)
setclipboard("loadstring(game:HttpGet('https://raw.githubusercontent.com/rrixh/lulaslollipopmobile/main/lollypophubmobilev2'))();")
]])
rnote("Lollypop Hub 🍭\nKopied Backpack Tool skript, paste in your executor")
end)

